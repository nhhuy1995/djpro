/*   Container, Logo
--------------------------------------------------*/
ul.jp-toggles {  position:absolute; text-align:center; top:-10px; right:12px
}

ul.jp-toggles li {
    font-size:13px;
    cursor: pointer;
}
ul.jp-toggles li.drop {
    position: relative;
}
ul.jp-toggles > li {
    display: inline-block;
}
ul.jp-toggles li a {
    line-height: 30px;
    padding: 8px 20px;
    color: #fff;
}

ul.jp-toggles ul li a:hover{  color:#B302CB !important}

.dropOut .triangle {
    width: 0;
    height: 0;
    position: absolute;
    border-left: 8px solid transparent;
    border-right: 8px solid transparent;
    border-top: 8px solid #000;
    bottom:-8px;
    left: 50%;
    margin-left: -8px;
}
.dropdownContain {
    position: absolute;
    z-index: 0;
    left: 50%;
    margin-left: -140px; /* half of width */
}
.dropOut {
    background:#000;
    position: absolute;
    bottom: -15px; display:none;
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    border-radius: 4px;
    -webkit-box-shadow: 0 1px 6px rgba(0,0,0,.15);
    -moz-box-shadow: 0 1px 6px rgba(0,0,0,.15);
    box-shadow: 0 1px 6px rgba(0,0,0,.15);
    width:80px;
}

.dropOut ul.jp-toggles {
    float: left;
    padding: 10px 0;
}

ul.jp-toggles li:hover a { color: #fff; }
ul.jp-toggles li:hover .dropOut { display:block;}


.play-list{overflow:hidden; font-family:Arial, serif;}
.play-list .clr{clear:both;}
.play-list .album{ height:232px; width:232px; display:block; margin:0 auto; padding:0; position:relative;}
.play-list .album img.cover{ height:232px; width:232px; display:block; margin:0; padding:6px; position:absolute; top:0; left:0; background:url('images/plate_album.png') 0 0 no-repeat; z-index:4;}
.play-list .album .glass{height:217px; display:block; margin:0; position:absolute; top:8px; right:80px; left:8px; background:url('images/plate_album.png') right -232px no-repeat; z-index:3;}
.play-list .album .record{height:213px; width:213px; display:block; position:absolute; top:10px; right:6px; background:url('images/plate_album.png') right -449px no-repeat; z-index:1;}

.rotate{
    -webkit-animation: mymove 1s infinite linear;
    -moz-animation: mymove 1s infinite linear;
    -o-animation: mymove 1s infinite linear;
    -ms-animation: mymove 1s infinite linear;
    animation: mymove 1s infinite linear;
}

@-webkit-keyframes mymove {
    0% { -webkit-transform: rotate(0deg);}
    100% { -webkit-transform: rotate(360deg);}
}
@-moz-keyframes mymove {
    0% { -moz-transform: rotate(0deg);}
    100% { -moz-transform: rotate(360deg);}
}
@-o-keyframes mymove {
    0% { -o-transform: rotate(0deg);}
    100% { -o-transform: rotate(360deg);}
}
@-ms-keyframes mymove {
    0% { -ms-transform: rotate(0deg);}
    100% { -ms-transform: rotate(360deg);}
}
@-keyframes mymove {
0% { transform: rotate(0deg);}
100% { transform: rotate(360deg);}
}


